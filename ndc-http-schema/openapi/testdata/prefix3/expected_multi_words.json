    "securitySchemes": {
      "app_key": {
        "type": "http",
        "header": "Authorization",
        "scheme": "bearer",
        "value": {
          "env": "APP_KEY_TOKEN"
        }
      },
      "user_key": {
        "type": "http",
        "header": "Authorization",
        "scheme": "bearer",
        "value": {
          "env": "USER_KEY_TOKEN"
        }
      }
    },
    "version": "1.2.2"
